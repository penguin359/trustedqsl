	$(MKDIR) -p dist/TrustedQSL/tqsl.app/Contents
	$(MKDIR) -p dist/TrustedQSL/tqsl.app/Contents/MacOS
	$(MKDIR) -p dist/TrustedQSL/tqsl.app/Contents/Resources
